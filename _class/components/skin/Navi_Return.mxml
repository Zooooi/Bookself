<?xml version="1.0" encoding="utf-8"?>
<s:SparkButtonSkin xmlns:fx="http://ns.adobe.com/mxml/2009" 
             xmlns:s="library://ns.adobe.com/flex/spark" 
             xmlns:fb="http://ns.adobe.com/flashbuilder/2009"
             minWidth="21" minHeight="21" 
             alpha.disabled="0.5">
     
    <fx:Metadata>
        <![CDATA[ 
        [HostComponent("spark.components.Button")]
        ]]>
    </fx:Metadata>
   
    <s:states>
        <s:State name="up" />
        <s:State name="over" />
        <s:State name="down" />
        <s:State name="disabled" />
    </s:states>
    <s:BitmapImage includeIn="up,down" width="100%" height="100%" source="@Embed('/embed/button/icon/top_r2_c2.png')" />
    <s:BitmapImage includeIn="over" source="@Embed('/embed/button/icon/top_r2_c2_s2.png')" width="100%" height="100%" />
</s:SparkButtonSkin>

<?xml version="1.0" encoding="utf-8"?>
<s:Group xmlns:fx="http://ns.adobe.com/mxml/2009" 
		 xmlns:s="library://ns.adobe.com/flex/spark" 
		 xmlns:mx="library://ns.adobe.com/flex/mx">
	<fx:Script>
		<![CDATA[
			import JsC.events.JEvent;
		]]>
	</fx:Script>
	<fx:Style source="/css/book.css"/>
	<s:BorderContainer styleName="NormalNumber" width="100%" height="100%" backgroundAlpha="0.5" borderVisible="false" backgroundColor="#000000">
	</s:BorderContainer>
	<s:VGroup width="100%" height="100%" gap="15" horizontalAlign="center" verticalAlign="middle">
		<s:Button styleName="NormalNumber" label="download" click="this.dispatchEvent(new JEvent(JEvent.DOWNLOAD))"/>
		<s:Button styleName="NormalNumber" label="cancel"
				  click="this.dispatchEvent(new JEvent(JEvent.CANCEL))"/>
		<!--<s:HGroup width="100%" height="100%" horizontalAlign="center" verticalAlign="middle">
			
		</s:HGroup>-->
	</s:VGroup>
	
</s:Group>

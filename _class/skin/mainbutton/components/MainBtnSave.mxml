<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:s="library://ns.adobe.com/flex/spark"
		xmlns:fx="http://ns.adobe.com/mxml/2009"
		width="33" height="29">
	<fx:Metadata>[HostComponent("spark.components.Button")]</fx:Metadata>
	<s:states>
		<s:State name="up"/>
		<s:State name="over"/>
		<s:State name="down"/>
		<s:State name="disabled"/>
	</s:states>
	<s:BitmapImage x="0" y="0" smooth="true" source="@Embed('skin/mainbutton/assets/images/Button_save_normal.png')"
				   visible.disabled="false"
				   visible.over="false"/>
	<s:BitmapImage x="0" y="0" smooth="true" source="@Embed('skin/mainbutton/assets/images/Button_save_over.png')"
				   visible.disabled="false"
				   visible.down="false"
				   visible.up="false"/>
	<s:BitmapImage includeIn="disabled,up" x="0" y="0" smooth="true"
				   source="@Embed('skin/mainbutton/assets/images/Button_save_active1.png')"
				   visible.up="false"/>
</s:Skin>

<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:s="library://ns.adobe.com/flex/spark"
		xmlns:fx="http://ns.adobe.com/mxml/2009"
		width="18" height="18">
	<fx:Metadata>[HostComponent("spark.components.Button")]</fx:Metadata>
	<s:states>
		<s:State name="up"/>
		<s:State name="over"/>
		<s:State name="down"/>
		<s:State name="disabled"/>
	</s:states>
	<s:BitmapImage x="0" y="0" smooth="true" source="@Embed('skin/mainbutton/assets/images/Button_Popup_Close.png')"
				   visible.disabled="false"
				   visible.over="false"
				   visible.up="false"/>
	<s:BitmapImage x="0" y="0" smooth="true"
				   source="@Embed('skin/mainbutton/assets/images/Button_PopupClose_ACTIVE.png')"
				   visible.disabled="false"
				   visible.down="false"
				   visible.over="false"/>
	<s:BitmapImage x="0" y="0" smooth="true"
				   source="@Embed('skin/mainbutton/assets/images/Button_PopupClose_OVER.png')"
				   visible.down="false"
				   visible.up="false">
		<s:filters>
			<s:BlurFilter includeIn="disabled" blurX="4.0" blurY="4.0" quality="2"/>
		</s:filters>
	</s:BitmapImage>
</s:Skin>

		width="100" height="19" >
	
	<fx:Metadata>
		[HostComponent("spark.components.HSlider")]
	</fx:Metadata> 
	
	<s:states>
		<s:State name="normal" />
		<s:State name="disabled" />
	</s:states>
	
	
	<s:Button id="track" left="0" right="0" top="4" bottom="4"
			  skinClass="skin.controls.components.HorizontalSliderSkinTrackSkin"
			  alpha.disabled="0.5"
			  alpha.normal="0"/>
	<s:Button id="thumb" x="0" skinClass="skin.controls.components.HorizontalSliderSkinThumbSkin"
			  verticalCenter="0"
			  alpha.disabled="0.5"/>
	
</s:Skin>

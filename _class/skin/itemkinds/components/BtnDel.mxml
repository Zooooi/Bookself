<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:s="library://ns.adobe.com/flex/spark"
		xmlns:fx="http://ns.adobe.com/mxml/2009"
		xmlns:d="http://ns.adobe.com/fxg/2008/dt"
		width="34" height="26">
	<fx:Metadata>[HostComponent("spark.components.Button")]</fx:Metadata>
	<s:states>
		<s:State name="up"/>
		<s:State name="over"/>
		<s:State name="down"/>
		<s:State name="disabled"/>
	</s:states>
	<s:BitmapImage smooth="true" source="@Embed('skin/itemkinds/assets/images/btn_del.png')"
				   x.disabled="0" y.disabled="0" alpha.disabled="0.5"
				   x.down="0" y.down="0"
				   visible.over="false" x.over="0" y.over="0">
		<s:filters>
			<s:BlurFilter includeIn="disabled" blurX="4.0" blurY="4.0" quality="2"/>
		</s:filters>
	</s:BitmapImage>
	<s:BitmapImage x="0" y="0" smooth="true" source="@Embed('skin/itemkinds/assets/images/btn_del.png')"
				   visible.disabled="false"
				   visible.down="false"
				   visible.up="false"/>
</s:Skin>
